<script>
    function AddToTable() {
        var txtDisable = $("#DisableName").val();
        var txtVal = $("#ValName").val();

        if (txtDisable == '' || txtVal == '') {
            Swal.fire('هشدار!', 'نام و مقدار را باید وارد کنید', 'warning');
        }
        else {
            $('#FeaturesTable tbody').append('<tr> <td>' + txtDisable + '</td>  <td>' + txtVal + '</td> <td> <button id="idFeatures" onclick="deleteRow(this)" class="btn btn-danger mb-3 me-2"> حذف </button> </td> </tr>');
            $("#DisableName").val("");
            $("#ValName").val("");
        }
    };
    function upTo(el, tagName) {
        tagName = tagName.toLowerCase();

        while (el && el.parentNode) {
            el = el.parentNode;
            if (el.tagName && el.tagName.toLowerCase() == tagName) {
                return el;
            }
        }
        return null;
    }

    function deleteRow(el) {
        var row = upTo(el, 'tr')
        if (row) row.parentNode.removeChild(row);
    }
</script>
