<script>
    // Multiple images preview in browser
    let fileInput = document.getElementById("file-input");
    let imageContainerModal = document.getElementById("PictureModal");
    let imageContainer = document.getElementById("Images");
    let numOfFiles = document.getElementById("num-of-files");
    let divclasses = ['col-xl-4', 'col-sm-6'];
    let aTagclasses = ['gallery-item', 'rounded-2'];
    let spanTagClasses = ['gallery-item-caption'];
    function preview() {
        imageContainer.innerHTML = "";
        numOfFiles.textContent = `${fileInput.files.length} Files Selected`;
        for (i of fileInput.files) {
            let reader = new FileReader();
            let div = document.createElement("div");
            let aTag = document.createElement("a");
            let spanTag = document.createElement("span");
            div.classList.add(...divclasses);
            aTag.classList.add(...aTagclasses);
